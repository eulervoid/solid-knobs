{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017", "dom"],
    "sourceMap": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "module": "es2020",
    "declaration": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ]
}